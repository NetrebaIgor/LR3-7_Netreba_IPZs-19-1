@model IEnumerable<Test.Models.InsuranceCOVID>


<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Видалення страхувань</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/List.css" />
</head>


<body>
    <div class="container">
      
        
  
               
         
        <h2>Страхування від COVID</h2>

        <table>
            <tr>
                <th>ПІБ</th>
                <th>Дата</th>


            </tr>

            @foreach (var item in Model)
            {
                <tr>
                <td>@item.NSF</td>
                <td>@item.Date</td>

                <th>
                    <form asp-controller="Home" asp-action="DeleteCOVID" asp-route-id="@item.Id" method="post">
                        <div class="form-group">
                            <input type="submit" class="btn btn-default" value="Видалити" />
                        </div>
                    </form>
                </th>
</tr>
            }
        </table>

       

    </div>
</body>